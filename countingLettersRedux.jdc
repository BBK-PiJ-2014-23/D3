boolean finished = false;
String str = System.console().readLine();
String letter = "";
String last = "";
while (!finished) {
	letter = System.console().readLine();
	if (letter.equals(last)) {
		finished = true;
	} else {
		int counter = 0;
		
		for (int i = 0; i < str.length(); i++) {
			if (letter.equals(str.substring(i, i + 1))) {
				counter++;
			}
		}
		println(counter);
		last = letter;
	}
}

